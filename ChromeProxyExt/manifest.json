{
    "version": "1.1.1",
    "manifest_version": 2,
    "default_locale": "en",
    "description": "__MSG_appDesc__",
    "name": "__MSG_appName__",
    "update_url": "http://127.0.0.1/update.xml",
    "icons": {
        "128": "images/on.png",
        "48":  "images/on48.png",
        "16":  "images/on16.png"
    },
    "permissions": [
        "proxy",
        "storage"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "commands": {
        "open-option": {
          "suggested_key": {
            "default": "Alt+O"},
          "description": "Open option page"
        }
    },
    "browser_action": {
        "default_icon": "images/off.png",
        "default_title": "__MSG_title__"
    },
    "minimum_chrome_version":"22.0.0"
}

